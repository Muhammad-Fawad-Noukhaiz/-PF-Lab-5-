#include <stdio.h>

int main() {
    
   int age;
    char citizenship[4]; // "yes" or "no"

    printf("Enter your age: ");
    scanf("%d", &age);

    printf("Are you a citizen? (yes/no): ");
    scanf("%s", citizenship);

    if (age >= 18 && (citizenship[0] == 'y' || citizenship[0] == 'Y')) {
        printf("You are eligible to vote.\n");
    } else {
        printf("You are not eligible to vote.\n");
    }

    return 0;
}
